<?xml version="1.0" encoding="utf-8"?>
<Rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:noNamespaceSchemaLocation="RuleSchema.xsd">
	<Rule Name="1000BASE-LX"
		  TestPort="Singlemode"
		  Units="dB"
		  LossThreshType="TotalLoss"
		  Description="Gigabit Ethernet LX(1310nm)"
		  CatType="Ethernet"
		  StdType="Application" Version="1.0.1">
		<LossThreshold WL="1310"
					   Min="0.0"
					   Max="4.5"></LossThreshold>
		<MaxLength Units="Meters">5000</MaxLength>
	</Rule>
</Rules>