<?xml version="1.0" encoding="utf-8"?>
<Rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:noNamespaceSchemaLocation="RuleSchema.xsd">
	<Rule Name="1000BASE-SX-50"
		  TestPort="Multimode"
		  Units="dB"
		  LossThreshType="TotalLoss"
		  Description="1000BASE-SX (850 nm) on (OM2) 50 μm fiber"
		  CatType="Ethernet"
		  FTA="OM2_50_125um"
		  StdType="Application" Version="1.0.1">
		<LossThreshold WL="850"
					   Min="0"
					   Max="3.6"></LossThreshold>
		<MaxLength Units="Meters">550</MaxLength>
	</Rule>
</Rules>