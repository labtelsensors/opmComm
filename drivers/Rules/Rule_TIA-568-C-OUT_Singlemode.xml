<?xml version="1.0" encoding="utf-8"?>
<Rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:noNamespaceSchemaLocation="RuleSchema.xsd">
	<Rule Name="TIA-568-C-OUT"
		  TestPort="Singlemode"
		  Units="dB"
		  LossThreshType="FiberLoss"
		  Description="TIA/EIA-568-C, outside plant, singlemode fiber."
		  UseType="Out"
		  CatType="TIA"
		  Rev="C"
		  StdType="Cabling" Version="1.0.1" 
		  LengthThreshType="Application">
		<LossThreshold WL="1310"
					   Min="0"
					   Max="0.5"></LossThreshold>
		<LossThreshold WL="1550"
					   Min="0"
					   Max="0.5"></LossThreshold>
		<MaxLength Units="Meters"></MaxLength>
		<LossPerSplice>0.3</LossPerSplice>
		<LossPerConnector>0.75</LossPerConnector>
	</Rule>
</Rules>