<?xml version="1.0" encoding="utf-8"?>
<Rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:noNamespaceSchemaLocation="RuleSchema.xsd">
	<Rule Name="10GBASE-LX4-SM"
		  TestPort="Singlemode"
		  Units="dB"
		  LossThreshType="TotalLoss"
		  Description="10GBASE-LX4 (1310 nm) on 10 μm fiber"
		  CatType="Ethernet"
		  StdType="Application" Version="1.0.1">
		<LossThreshold WL="1310"
					   Min="0"
					   Max="6.2"></LossThreshold>
		<MaxLength Units="Meters">10000</MaxLength>
	</Rule>
</Rules>