<?xml version="1.0" encoding="utf-8"?>
<Template version="1.0.0.0">
	<Body>
		<Div RequiredElement="False" Positioning="Content" Alignment="Top" Width="100" Padding="5,5,5,5" BackColor="LightGray">
			<Div Name="Fiber Optic Report Block" Positioning="Content" Padding="2,2,2,2">
				
				<Div Positioning="Percentage" Width="50">
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Num. fibra:" ID="0" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="FiberNumber" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Lunghezza d&apos;onda:" ID="1" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="Wavelength" FontSize="12" />
							<Label Text="nm" ID="2" Alignment="Left" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="ID cavo:" ID="3" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="CableID" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Data della prova:" ID="4" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="TimeStamp" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Società:" ID="5" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="CompanyName" FontSize="12" />
						</Div>
					</Div>
				</Div>
				<Div Positioning="Percentage" Width="50">
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Estremità 1:" ID="6" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="EndOneSite" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Estremità 2" ID="7" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="EndTwoSite" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="OTDR posizionato in:" ID="8" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="OTDRLocation" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Testato da:" ID="9" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="Operator" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Operatore 2:" ID="10" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="Operator2" FontSize="12" />
						</Div>
					</Div>

				</Div>
			</Div>
		</Div>
		
		<Template FileName="CommentBlock.xml" />
		
		<Div RequiredElement="False" Positioning="Content" Alignment="Top" Width="100" Padding="5,5,5,5" BackColor="LightGray">
			<Div Positioning="Content" Padding="2,2,2,2">
				
				<Div Positioning="Percentage" Width="33">
					
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Modulo:" ID="11" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="MainFrameID" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Porta:" ID="12" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="FiberType" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Campo:" ID="13" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="DisplayRange" FontSize="12" />
							<Data DataSource="OTDRInfo" Field="DisplayRange.DistanceUnits" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Ampiezza impulso:" ID="14" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="PulseWidth" FontSize="12" />
							<Label Text=" ns" ID="15" Alignment="Left" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="30">
							<Label Text="Medie:" ID="16" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="NumberOfAverages" FontSize="12" />
						</Div>
					</Div>
					
				</Div>
				
				<Div Positioning="Percentage" Width="33">
					
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="45">
							<Label Text="Coef. retrodiffusione:" ID="17" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="BackscatterCoef" FontSize="12" />
							<Label Text="dB" ID="18" Alignment="Left" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="45">
							<Label Text="Soglia attenuazione:" ID="19" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="LossThreshold" FontSize="12" />
							<Label Text="dB" ID="20" Alignment="Left" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="45">
							<Label Text="Soglia rifl.:" ID="21" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="ReflThreshold" FontSize="12" />
							<Label Text="dB" ID="22" Alignment="Left" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="45">
							<Label Text="Soglia estremità:" ID="23" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="EndThreshold" FontSize="12" />
							<Label Text="dB" ID="24" Alignment="Left" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="45">
							<Label Text="Indice gruppo:" ID="25" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="IOR" FontSize="12" />
						</Div>
					</Div>
					
				</Div>
				
				<Div Positioning="Fill">
					
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="40">
							<Label Text="Cursore A:" ID="26" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="Cursor.A.Location.InCurrentUnits" FontSize="12" Format="0.00" />
							<Label Text=" " FontSize="12" />
							<Data DataSource="OTDRInfo" Field="Cursor.A.Location.DistanceUnits" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="40">
							<Label Text="Cursore B:" ID="27" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data DataSource="OTDRInfo" Field="Cursor.B.Location.InCurrentUnits" FontSize="12" Format="0.00" />
							<Label Text=" " FontSize="12" />
							<Data DataSource="OTDRInfo" Field="Cursor.B.Location.DistanceUnits" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="40">
							<Label Text="Distanza B-A:" ID="28" FontSize="12" />
						</Div>
						<Div Positioning="Fill">
							<DataCalc FontSize="12" Format="0.00">
								<Data RenderOption="SkipElement" Name="A" DataSource="OTDRInfo" Field="Cursor.A.Location.InCurrentUnits" />
								<Data RenderOption="SkipElement" Name="B" DataSource="OTDRInfo" Field="Cursor.B.Location.InCurrentUnits" />
								<DataCalculation RenderOption="SkipElement" Function="'B'-'A'" />
							</DataCalc>
							<Label Text=" " FontSize="12" />
							<Data DataSource="OTDRInfo" Field="Cursor.B.Location.DistanceUnits" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="40">
							<Label Text="Atten. B-A:" ID="29" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<DataCalc FontSize="12" Format="0.00">
								<Data RenderOption="SkipElement" Name="A" DataSource="OTDRInfo" Field="Cursor.A.Loss" />
								<Data RenderOption="SkipElement" Name="B" DataSource="OTDRInfo" Field="Cursor.B.Loss" />
								<DataCalculation RenderOption="SkipElement" Function="('B'-'A')/1000" />
							</DataCalc>
							<Label Text="dB" ID="30" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top" >
						<Div Positioning="Percentage" Width="40">
							<Label Text="Atten. B-A/km:" ID="31" FontSize="12" />
						</Div>
						<Div Positioning="Fill" >
							<Data FontSize="12" DataSource="OTDRInfo" Format="0.00" Field="LSAAttenuation" />
							<Label Text="dB/km" ID="32" FontSize="12" />
						</Div>
					</Div>

				</Div>
				
			</Div>
		</Div>
	</Body>
</Template>
