<?xml version="1.0" encoding="utf-8"?>
<Template version="1.0.0.0">
	<Body>
		<PageSettings>
			<Padding>40,40,40,40</Padding>
			<Positioning>Absolute</Positioning>
			<BackColor>255,255,255</BackColor>
			<BorderStyle>Radius</BorderStyle>
		</PageSettings>
		<Div Alignment="Top" Positioning="Percentage" Height="10" Padding="0,20,0,0">
			<Label Alignment="HorizCenter" Text="Synthèse de la route" ID="0" FontSize="36" />
		</Div>

		<Div Alignment="Top" Positioning="Content" Padding="0,20,0,0" >
			<Label Alignment="Left" Text="Résultats:" ID="1" FontSize="24" />
		</Div>

		<Div Alignment="Top" Positioning="Content" Padding="20,20,0,0" >
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Câbles:" ID="2" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="CableCount" FontSize="20" />
				</Div>
				<Div Alignment="Left" Positioning="Fill">
					<Label Alignment="Left" Text=" -"  FontSize="20" />
					<Data Alignment="Left" DataSource="User" Field="CableCountWithData" FontSize="20" />
					<Label Alignment="Left" Text="avec des résultats" ID="3"  FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Fibres:" ID="4" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="FiberCount" FontSize="20" />
				</Div>
				<Div Alignment="Left" Positioning="Fill">
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="Traces:" ID="5" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="TraceCount" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Monomode Traces:" ID="6" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="SingleModeTraces" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Multimode Traces:" ID="7" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="MultiModeTraces" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="OLTS / OPM Résultats:" ID="9" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="OLTSOPMResults" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="OLTS fibre Lecture:" ID="10" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="OLTSFibers" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="OPM fibre Lecture:" ID="11" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="OPMFibers" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="Inconnu Traces:" ID="9" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="Images" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="Les longueurs d'onde monomodes testée:" ID="12" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="20">
					<Data Alignment="Right" DataSource="User" Field="SingleModeWaveLengths" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Les longueurs d'onde multimodes testée:" ID="13" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="20">
					<Data Alignment="Right" DataSource="User" Field="MultiModeWaveLengths" FontSize="20" />
				</Div>
			</Div>
		</Div>
	</Body>
</Template>
