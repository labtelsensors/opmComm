<?xml version="1.0" encoding="utf-8"?>
<Template version="1.0.0.0">
	<Body>
		<PageSettings>
			<Padding>40,40,40,40</Padding>
			<Positioning>Absolute</Positioning>
			<BackColor>255,255,255</BackColor>
			<BorderStyle>Radius</BorderStyle>
		</PageSettings>
		<Div Alignment="Top" Positioning="Percentage" Height="10" Padding="0,20,0,0">
			<Label Alignment="HorizCenter" Text="Сводка по кабелю" ID="0" FontSize="36" />
		</Div>

		<Div Alignment="Top" Positioning="Content" Padding="0,20,0,0" >
			<Label Alignment="Left" Text="Результаты:" ID="1" FontSize="24" />
		</Div>

		<Div Alignment="Top" Positioning="Content" Padding="20,20,0,0" >
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Волокна:" ID="2" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="FiberCount" FontSize="20" />
				</Div>
				<Div Alignment="Left" Positioning="Fill">
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="Трассы:" ID="3" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="TraceCount" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Одномодовые трассы:" ID="4" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="SingleModeTraces" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Многомодовые трассы:" ID="5" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="MultiModeTraces" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="Результаты OLTS / OPM:" ID="7" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="OLTSOPMResults" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Результаты измерений OLTS" ID="8" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="OLTSFibers" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Результаты измерений OPM:" ID="9" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="OPMFibers" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="Изображения концов волокон:" ID="9" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="10">
					<Data Alignment="Right" DataSource="User" Field="Images" FontSize="20" />
				</Div>
			</Div>

			<Div Alignment="Top" Positioning="Content" Padding="0,10,0,0">
				<Label Alignment="Left" Positioning="Percentage" Text="Протестировано одномодовых длин волн:" ID="10" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="20">
					<Data Alignment="Right" DataSource="User" Field="SingleModeWaveLengths" FontSize="20" />
				</Div>
			</Div>
			<Div Alignment="Top" Positioning="Content">
				<Label Alignment="Left" Positioning="Percentage" Text="Протестировано многомодовых длин волн:" ID="11" Width="50" FontSize="20" />
				<Div Alignment="Left" Positioning="Percentage" Width="20">
					<Data Alignment="Right" DataSource="User" Field="MultiModeWaveLengths" FontSize="20" />
				</Div>
			</Div>
		</Div>
	</Body>
</Template>
