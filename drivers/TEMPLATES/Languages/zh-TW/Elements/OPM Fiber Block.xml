						<Div Positioning="Percentage" Alignment="Left" Width="30">
							<Label Text="光纜 ID:" ID="0" Alignment="Left" FontSize="12" />
						</Div>
						<Div Positioning="Fill" Alignment="Left">
							<Data DataSource="OPMInfo" Field="CableID" FontSize="12" Text="       " />
						</Div>
					</Div>
					<Div Positioning="Content" Alignment="Top">
						<Div Positioning="Percentage" Alignment="Left" Width="30">
							<Label Text="終點 1:" ID="1" Alignment="Left" FontSize="12" />
						</Div>
						<Div Positioning="Fill" Alignment="Left">
							<Data DataSource="OPMInfo" Field="End1" FontSize="12" Text="       " />
