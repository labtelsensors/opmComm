						<Div Positioning="Fill" Alignment="Left">
							<Data DataSource="OPMInfo" Index="0" Field="TimeStamp" FontSize="12" Text="       " />
						</Div>
					</Div>
				</Div>
				<Div Positioning="Percentage" Alignment="Left" Width="50">
					<Div Positioning="Content" Alignment="Top">
						<Div Positioning="Percentage" Alignment="Left" Width="40">
							<Label Text="測試者:" ID="4" Alignment="Left" FontSize="12" />
						</Div>
						<Div Positioning="Fill" Alignment="Left">
							<Data DataSource="OPMInfo" Field="Operator1" FontSize="12" Text="       " />
