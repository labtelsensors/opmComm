								<Data DataSource="OTDRInfo" Index="0" Field="DistanceUnits" Alignment="Left" FontSize="12" BorderStyle="None" />
							</Div>
						</Div>
					</Div>
				</Div>
				<Div Positioning="Percentage" Alignment="Top" Height="55" >
					<EventLine Index="0" X="5" Y="30" X2="95" Y2="30" BackColor="Black" ForeColor="Blue" FontSize="10" />
				</Div>
			</Div>
		</Div>
