							<Label Text="端点 1" ID="1" Alignment="Left" Padding="0,2,2,2" FontSize="12" />
						</Div>
						<Div Positioning="Content" Alignment="Top"  >
							<Data DataSource="OTDRInfo" Index="0" Field="EndOneSite"  Padding="0,2,2,2" FontSize="12" />
						</Div>
					</Div>
					<Div Positioning="Percentage" Alignment="Right" >
						<Div Positioning="Content" Alignment="Top"  >
							<Label Text="端点 2" ID="2" Alignment="Right" FontSize="12" />
